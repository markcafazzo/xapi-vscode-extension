{
  "name": "xapi-tools",
  "displayName": "xapi-tools",
  "description": "xAPI toolkit for statement validation, configuration, injection of statement/xapiwrapper into courseware packages, and more!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:xapi-tools.xapivalidate",
    "onCommand:xapi-tools.time"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xapi-tools.xapivalidate",
        "title": "xapivalidate"
      },
      {
        "command": "xapi-tools.time",
        "title": "time"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "xapi-validation": "^2.1.4"
  }
}
